, { useState, useEffect } from \"react\";\r\nimport axios from \"axios\";\r\nimport ScreenTop from \"../mobileComp/ScreenTop\";\r\nimport \"./DocumentDetails.css\";\r\nimport \"../../App.css\";\r\n//import ProfileBar from \"../Profile/ProfileBar\";\r\nimport { url } from \"../../GlobalUrl\";\r\nimport { Col, Row, Button } from \"reactstrap\";\r\nimport DocumentDetailsModal from \"./DocumentDetailsModal\";\r\nimport { GrAdd } from \"react-icons/gr\";\r\nimport DisplayBar from \"../Navlinks/DisplayBar\";\r\nimport Top from \"../Top\";\r\nimport { saveAs } from \"file-saver\";\r\nimport CommonModal from \"../CommonModal/CommonModal\";\r\n\r\nconst DocumentDetails = (props) => {\r\n  const [docContents, setDocContents] = useState({});\r\n  const [imagePath, setImagePath] = useState(\"\");\r\n  const [objectData, setObjectData] = useState({});\r\n  const [modal, setModal] = useState(false);\r\n  const [docName, setDocName] = useState(\"\");\r\n  const [projectId, setProjectId] = useState(\r\n    localStorage.getItem(\"activeProjectId\")\r\n  );\r\n  const [docIndex, setDocIndex] = useState(0);\r\n  const [projectData, setProjectData] = useState([]);\r\n  const [subProjectData, setSubProjectData] = useState({});\r\n  const [show, setShow] = useState(false);\r\n  const [subprojectId, setSubProjectId] = useState(1);\r\n  const [csvlink, setcsvlink] = useState(\"\");\r\n  const [action, setAction] = useState(\"\");\r\n  // const [formNumber, setFormNumber] = useState(0);\r\n  const token = localStorage.getItem(\"token\");\r\n  const documentId = localStorage.getItem(\"documentId\");\r\n  const orgId = localStorage.getItem(\"orgId\");\r\n  const [docCount, setdocCount] = useState();\r\n  const [commonModal, setCommonModal] = useState(false);\r\n  const [msg, setMsg] = useState(\"\");\r\n\r\n  const commonToggle = () => setCommonModal(!commonModal);\r\n\r\n  const toggle = () => setModal(!modal);\r\n  useEffect(() => {\r\n    documentDetailsApi();\r\n  }, [docName]);\r\n\r\n  useEffect(() => {\r\n    exportDocumentApi();\r\n  }, []);\r\n\r\n  useEffect(() => {\r\n    subProjectListApi(action);\r\n  }, [projectId]);\r\n\r\n  async function documentDetailsApi() {\r\n    await axios\r\n      .get(url + `/api/document/${documentId}/`, {\r\n        headers: {\r\n          Authorization: `Bearer ${token}`,\r\n        },\r\n      })\r\n      .then((res) => {\r\n        // console.log(\"DocumentDetails\", res.data);\r\n        // console.log(res.data.document_name);\r\n        setImagePath(res.data.file);\r\n        setDocName(res.data.document_name);\r\n\r\n        // console.log(res.data.document_content);\r\n        // let c  = JSON.stringify(res.data.document_content);\r\n\r\n        // console.log(JSON.parse(res.data.document_content[0]));\r\n        //console.log(\"IMAGEPATH\",res.data.file);\r\n        setDocContents(JSON.parse(res.data.document_content));\r\n        setdocCount(JSON.parse(res.data.document_content).length - 1);\r\n        // console.log(\"JSON\", JSON.parse(res.data.document_content).length-1);\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  }\r\n\r\n  async function documentDeleteApi() {\r\n    await axios\r\n      .delete(url + `/api/document/${documentId}/`, {\r\n        headers: {\r\n          Authorization: `Bearer ${token}`,\r\n        },\r\n      })\r\n      .then((res) => {\r\n        setMsg(\"Document deleted successfully!\");\r\n        setCommonModal(true);\r\n        // alert(\"Document deleted successfully !\");\r\n        window.location.replace(\"/document-list\");\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  }\r\n\r\n  async function editDocNameApi(data) {\r\n    await axios\r\n      .patch(\r\n        url + `/api/document/${documentId}/`,\r\n        { document_name: data },\r\n        {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        }\r\n      )\r\n      .then((res) => {\r\n        setMsg(\"Document nam