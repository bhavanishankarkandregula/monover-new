ubprojectId,\r\n          document_id: localStorage.getItem(\"documentId\"),\r\n        },\r\n        {\r\n          headers: {\r\n            Authorization: `Bearer ${token}`,\r\n          },\r\n        }\r\n      )\r\n      .then((res) => {\r\n        // console.log(res.data);\r\n        setMsg(res.data.message);\r\n        setCommonModal(true);\r\n        // alert(res.data.message);\r\n        window.location.replace(\"/document-list\");\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n      });\r\n  };\r\n\r\n  const copyApi = async () => {\r\n    await axios\r\n      .post(\r\n        `${url}/api/copy/document/`,\r\n        {\r\n          subproject_id: subprojectId,\r\n          document_id: localStorage.getItem(\"documentId\"),\r\n        },\r\n        {\r\n          headers: {\r\n            Authorization: `Bearer ${token}`,\r\n          },\r\n        }\r\n      )\r\n      .then((res) => {\r\n        setMsg(res.data.message);\r\n        setCommonModal(true);\r\n        // alert(res.data.message);\r\n        // console.log(res.data);\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n      });\r\n  };\r\n\r\n  async function starDoc() {\r\n    localStorage.getItem(\"orgId\") &&\r\n      localStorage.getItem(\"documentId\") &&\r\n      (await axios\r\n        .post(\r\n          `${url}/api/starred/`,\r\n          {\r\n            organisation_id: `${localStorage.getItem(\"orgId\")}`,\r\n            documents: `${localStorage.getItem(\"documentId\")}`,\r\n          },\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          }\r\n        )\r\n        .then((res) => {\r\n          // console.log(res.data);\r\n          setMsg(res.data[0].message);\r\n          setCommonModal(true);\r\n          // alert(res.data[0].message);\r\n        })\r\n        .catch((e) => {\r\n          setMsg(e.message);\r\n          setCommonModal(true);\r\n          // alert(e.message);\r\n        }));\r\n  }\r\n  async function ExportToCSV(index) {\r\n    localStorage.getItem(\"documentId\") &&\r\n      (await axios\r\n        .get(`${url}/api/export/${documentId}/${index}/csv/`)\r\n        .then((res) => {\r\n          // console.log(res.data);\r\n          saveAs(\r\n            `${url}/api/export/${documentId}/${index}/csv`,\r\n            `form-${index + 1}.csv`\r\n          );\r\n        })\r\n        .catch((e) => {\r\n          alert(e.message);\r\n        }));\r\n  }\r\n  async function ExportToXLS(index) {\r\n    localStorage.getItem(\"documentId\") &&\r\n      (await axios\r\n        .get(`${url}/api/export/${documentId}/${index}/xls/`)\r\n        .then((res) => {\r\n          // console.log(res.data);\r\n          saveAs(\r\n            `${url}/api/export/${documentId}/${index}/xls`,\r\n            `form-${index + 1}.xls`\r\n          );\r\n        })\r\n        .catch((e) => {\r\n          setMsg(e.message);\r\n          setCommonModal(true);\r\n          // alert(e.message);\r\n        }));\r\n  }\r\n\r\n  async function removeStarDoc() {\r\n    localStorage.getItem(\"documentId\") &&\r\n      localStorage.getItem(\"orgId\") &&\r\n      (await axios\r\n        .post(\r\n          `${url}/api/remove/starred/`,\r\n          {\r\n            organisation_id: `${localStorage.getItem(\"orgId\")}`,\r\n            documents: `${localStorage.getItem(\"documentId\")}`,\r\n          },\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          }\r\n        )\r\n        .then((res) => {\r\n          // console.log(res.data);\r\n          setMsg(res.data[0].message);\r\n          setCommonModal(true);\r\n          // alert(res.data[0].message);\r\n        })\r\n        .catch((e) => {\r\n          setMsg(e.message);\r\n          setCommonModal(true);\r\n          // alert(e.message);\r\n        }));\r\n  }\r\n  // console.log(\"CSVLINK\", csvlink);\r\n\r\n  return (\r\n    <div className=\"main\" style={{ width: \"100%\", height: \"100%\" }}>\r\n      <ScreenTop />\r\n      <Top />\r\n      <DisplayBar\r\n        title={docName}\r\n        id={documentId}\r\n        handleDelete={handleDelete}\r\n        setDocName={setDocName}\r\n        editDocNameApi={editDocNameApi}\r\n        starDoc={starDoc}\r\n        removeStarDoc={removeStarDoc}\r\n        isStarDoc={localStorage.getItem(\"docStarred\")}\r\n      />\r\n      <div>\r\n        <Row>\r\n          <Col md={6}>\r\n            <div className=\"DocumentType\">\r\n              {docName.split(\".\")[docName.split(\".\").length - 1] === \"pdf\" ? (\r\n                <iframe\r\n                  className=\"PDFIMG\"\r\n                  title=\"Documents\"\r\n                  src={`https://docs.google.com/gview?url=${imagePath}&embedded=true`}\r\n                  //style={{width:\"20vw\",height:\"50vh\"}}\r\n                />\r\n              ) : (\r\n                <img\r\n                  src={imagePath}\r\n                  alt=\"IMG\"\r\n                  className=\"PDFIMG\"\r\n                  style={{ borderColor: \"white\", borderRadius: \"15px\" }}\r\n                />\r\n              )}\r\n\r\n              {/* <img src= { docName.split(\".\")[docName.split(\".\").length -1]===\"pdf\" ?  pdfIcon  :  imagePath }\r\n               alt=\"IMG\" /> */}\r\n            </div>\r\n            <>\r\n              <div className=\"MoveCopy\">\r\n                <div className=\"Details_Form_button\">\r\n                  <div className=\"Details_Form_group_button\">\r\n                    <button\r\n                      style={{\r\n                        backgroundColor:\r\n                          action === \"move\" ? \"rgb(232, 232, 232)\" : \"#0f7cf9\",\r\n                        color: action === \"move\" ? \"black\" : \"white\",\r\n                        border:\r\n                          action === \"move\"\r\n                            ? \"1px solid #e8e8e8\"\r\n                            : \"1px solid #007fff\",\r\n  