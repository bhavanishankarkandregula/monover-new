filePath} width={80} alt=\"file\" />\r\n    else if (extension === \"pdf\")\r\n      return <img src={PdfIcon} width={80} alt=\"file\" />\r\n    else if (extension === \"doc\" || extension === \"docx\")\r\n      return <img src={WordIcon} width={80} alt=\"file\" />\r\n    else if (extension === \"txt\")\r\n      return <img src={TextIcon} width={80} alt=\"file\" />\r\n    return <img src={InvalidIcon} width={63} alt=\"file\" />\r\n  }\r\n\r\n  const document_content =\r\n    \"document_content\" in file ? JSON.parse(file.document_content) : []\r\n\r\n  return (\r\n    <div className=\"col-3\">\r\n      <div title={file.document_name} className=\"p-2 b-container \">\r\n        <div>\r\n          <FileIcon />\r\n        </div>\r\n        <div style={{ position: \"absolute\", marginLeft: \"70px\" }}>\r\n          <AiFillEye\r\n            style={{ cursor: \"pointer\" }}\r\n            onClick={() => setLgShow(true)}\r\n            size={20}\r\n          />\r\n        </div>\r\n        <div />\r\n        <div className=\"text-left lead small mt-4\">\r\n          {file.document_name.length < 20\r\n            ? file.document_name\r\n            : file.document_name.slice(0, 20) + \"...\"}\r\n        </div>\r\n      </div>\r\n      <Modal\r\n        size=\"lg\"\r\n        show={lgShow}\r\n        onHide={() => setLgShow(false)}\r\n        aria-labelledby=\"example-modal-sizes-title-lg\"\r\n      >\r\n        <Modal.Header closeButton>\r\n          <Modal.Title id=\"example-modal-sizes-title-lg\">\r\n            File preview\r\n          </Modal.Title>\r\n        </Modal.Header>\r\n        <Modal.Body>\r\n          <div className=\"row\">\r\n            <div className=\"col-7\">\r\n              {extension === \"jpg\" ||\r\n              extension === \"jpeg\" ||\r\n              extension === \"png\" ? (\r\n                <img\r\n                  src={filePath}\r\n                  style={{ width: \"100%\" }}\r\n                  alt={file.document_name}\r\n            